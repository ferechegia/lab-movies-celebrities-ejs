<h2>Add new celebrities</h2>

<form action="/celebrities/create" method="POST">
  <label>
    Name:
    <input type="text" name="name" />
  </label>

  <label>
    Occupation:
    <input type="text" name="occupation" />
  </label>

  <label>
    catchPhrase:
    <input type="text" name="catchPhrase" />
  </label>
  <button type="submit">New celebrity</button>
</form>
